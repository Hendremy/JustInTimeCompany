@model IEnumerable<FlightPath>

@{
    ViewData["Title"] = "Planning de vols";
}

<h1>Planning de vols</h1>

<p>
    <a class="btn btn-primary" asp-action="Create">Nouveau Vol<i class="bi bi-plus"></i></a>
</p>

<div id="paths" class="">
        @foreach (var path in Model) {
                <partial name="_PathRow" model=path/>
        }
</div>

@section Scripts{
    @{await Html.RenderPartialAsync("_BootstrapScriptsPartial");}
}